{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "sale",
        "supply ",
        "return"
      ],
      "required": true,
      "default": "sale"
    },
    "date": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date",
      "required": true
    },
    "quantity": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 1
    },
    "transaction": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::article.article",
      "inversedBy": "products"
    },
    "seller": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "transactions"
    },
    "TransactionStatus": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "in progress",
        "validated",
        "refused",
        "canceled"
      ],
      "required": true,
      "default": "in progress"
    }
  }
}
